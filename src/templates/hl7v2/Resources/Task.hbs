{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "Task",
        "id":"{{ID}}",
        {{#if ORC-1}},
            "status":"requested",
        {{/if}},
        {{#if ORC-1}},
            "status":"cancelled",
        {{/if}},
        {{#if ORC-1}},
            "intent":"order",
        {{/if}},
        "code":
        {
            "coding":
            [
                {
                    {{#if ORC-1}},
                        "code":"fulfill",
                    {{/if}},
                    {{#if ORC-1}},
                        "system":"http://hl7.org/fhir/CodeSystem/task-code",
                    {{/if}},
                },
            ],
        },
        {{#each (getFieldRepeats ORC-7)}}
            {{>DataType/TQTask.hbs TQ=this}},
        {{/each}}
        "authoredOn":"{{formatAsDateTime ORC-9}}",
        "lastModified":"{{formatAsDateTime ORC-9}}",
    },
    "request":{
        "method":"PUT",
        "url":"Task/{{ID}}",
    },
},
