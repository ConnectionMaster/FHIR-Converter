{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "Organization",
        "id":"{{ID}}",
        {{>DataType/HDOrganization.hbs HD=MSH-4}},
        {{>DataType/HDMSH_6.hbs HD=MSH-6}},
        "address":
        [
            {
                "country":{{>DataType/_string.hbs mapping="CodeSystem/Countries.hbs" inCode=MSH-17 }},
            },
            { {{>DataType/XAD.hbs XAD=OBX-24}} },
            {{#if OBX-25}},
                { {{>DataType/XAD.hbs XAD=OBX-24}} },
            {{/if}},
            {{#each (getFieldRepeats ORC-22)}}
                { {{>DataType/XAD.hbs XAD=this}} },
            {{/each}}
        ],
        {{#if (not MSH-21)}},
            {{>DataType/HDOrganization.hbs HD=MSH-4}},
        {{/if}},
        {{>DataType/HDOrganization.hbs HD=MSH-22}},
        {{>DataType/XONOrganization.hbs XON=ROL-14}},
        "contact":
        [
            {
                "name":
                {
                    {{#each (getFieldRepeats NK1-30)}}
                        {{>DataType/XPN.hbs XPN=this}},
                    {{/each}}
                },
                "telecom":
                [
                    {{#each (getFieldRepeats NK1-31)}}
                        { {{>DataType/XTN.hbs XTN=this}} },
                    {{/each}}
                ],
                "address":
                {
                    {{#each (getFieldRepeats NK1-32)}}
                        {{>DataType/XAD.hbs XAD=this}},
                    {{/each}}
                },
            },
        ],
        "type":
        [
            { {{>DataType/CWECodeableConcept.hbs mapping="CodeSystem/OrganizationUnitType.hbs" CWE=PRT-7}} },
        ],
        {{#each (getFieldRepeats PRT-8)}}
            {{>DataType/XONOrganization.hbs XON=this}},
        {{/each}}
        "telecom":
        [
            {{#each (getFieldRepeats ORC-23)}}
                { {{>DataType/XTN.hbs XTN=this}} },
            {{/each}}
        ],
        {{#each (getFieldRepeats RXA-17)}}
            {{>DataType/CWEOrganization.hbs CWE=this}},
        {{/each}}
    },
    "request":{
        "method":"PUT",
        "url":"Organization/{{ID}}",
    },
},
