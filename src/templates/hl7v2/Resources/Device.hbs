{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "Device",
        "id":"{{ID}}",
        "identifier":
        [
            {{#each (getFieldRepeats OBX-18)}}
                { {{>DataType/EIIdentifier.hbs EI=this}} },
            {{/each}}
            {{#each (getFieldRepeats PRT-10)}}
                { {{>DataType/EIIdentifier.hbs EI=this}} },
            {{/each}}
        ],
        "manufactureDate":"{{formatAsDateTime PRT-17}}",
        "expirationDate":"{{formatAsDateTime PRT-18}}",
        "lotNumber":"{{PRT-19}}",
        "serialNumber":"{{PRT-20}}",
    },
    "request":{
        "method":"PUT",
        "url":"Device/{{ID}}",
    },
},
