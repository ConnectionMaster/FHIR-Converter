{
    "fullUrl":"urn:uuid:{{ID}}",
    "resource":{
        "resourceType": "PractitionerRole",
        "id":"{{ID}}",
        {{>DataType/XCNPractitionerRole.hbs XCN=PD1-4}},
        "identifier":
        [
            { {{>DataType/EIIdentifier.hbs EI=ROL-1}} },
            {
                {{>DataType/PLN.hbs PLN=PRT-24}},
            },
        ],
        "code":
        [
            { {{>DataType/CWECodeableConcept.hbs mapping="CodeSystem/Role.hbs" CWE=ROL-3}} },
        ],
        {{#each (getFieldRepeats ROL-4)}}
            { {{>DataType/CWECodeableConcept.hbs mapping="CodeSystem/Participation.hbs" CWE=this}} },
        {{/each}}
        "period":
        {
            "start":"{{formatAsDateTime ROL-5}}",
            "start":"{{formatAsDateTime PRT-11}}",
            "end":"{{formatAsDateTime ROL-6}}",
            "end":"{{formatAsDateTime PRT-12}}",
        },
        "telecom":
        [
            {{#each (getFieldRepeats ROL-12)}}
                { {{>DataType/XTN.hbs XTN=this}} },
            {{/each}}
            {{#each (getFieldRepeats PRT-15)}}
                { {{>DataType/XTN.hbs XTN=this}} },
            {{/each}}
        ],
        {{>DataType/XCNPractitionerRole.hbs XCN=OBX-25}},
        {{#each (getFieldRepeats PRT-5)}}
            {{>DataType/XCNPractitionerRole.hbs XCN=this}},
        {{/each}}
        "specialty":
        [
            { {{>DataType/CWECodeableConcept.hbs CWE=PRT-6}} },
        ],
        {{#each (getFieldRepeats ORC-12)}}
            {{>DataType/XCNPractitionerRole.hbs XCN=this}},
        {{/each}}
        {{#if ROL-13}},
            "location":
            {
                "reference": "Location/{{generateUUID ROL-13}}",
            }
        {{/if}},
        {{>DataType/NDLPractitionerRole.hbs NDL=OBR-32}},
        {{#each (getFieldRepeats OBR-34)}}
            {{>DataType/NDLPractitionerRole.hbs NDL=this}},
        {{/each}}
        {{#each (getFieldRepeats OBR-35)}}
            {{>DataType/NDLPractitionerRole.hbs NDL=this}},
        {{/each}}
    },
    "request":{
        "method":"PUT",
        "url":"PractitionerRole/{{ID}}",
    },
},
